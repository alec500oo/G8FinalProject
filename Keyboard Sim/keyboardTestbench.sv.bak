`include "../keyboardDriver.sv"
module keyboardTestbench ();
    logic clk, reset_n, data;
    logic[7:0] out_code;

    KeyboardDriver dut(.clk(clk),
                       .data(data),
                       .reset_n(reset_n),
                       .code(out_code));
    initial begin
        clk = 0;
        reset_n = 0;
        data = 0;
        out_code = 0;
    end

initial begin
    automatic logic[10:0] testDat = 11'b00011100001;
    for (int i = 10; i >= 0; i--) begin
        //@(posedge clk);
        data = testDat[i];
    end
end


    always
        #5 clk = !clk;

    initial
        #100 $finish;

endmodule